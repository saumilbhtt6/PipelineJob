pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Development') {
            steps {
                echo 'Developing'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
        stage('Prod') {
            steps {
                echo 'Production'
            }
        }
    }
}
